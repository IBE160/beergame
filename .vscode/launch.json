{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Process API Requests (Default)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/.logging/process-api-requests.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      "python": "${workspaceFolder}/.logging/.venv/Scripts/python.exe",
      "cwd": "${workspaceFolder}",
      "env": {}
    },
    {
      "name": "Python: Process API Requests (No Clear)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/.logging/process-api-requests.py",
      "args": ["--no-clear"],
      "console": "integratedTerminal",
      "justMyCode": true,
      "python": "${workspaceFolder}/.logging/.venv/Scripts/python.exe",
      "cwd": "${workspaceFolder}",
      "env": {}
    },
    {
      "name": "Python: Process API Requests (Verbose)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/.logging/process-api-requests.py",
      "args": ["--verbose"],
      "console": "integratedTerminal",
      "justMyCode": true,
      "python": "${workspaceFolder}/.logging/.venv/Scripts/python.exe",
      "cwd": "${workspaceFolder}",
      "env": {}
    },
    {
      "name": "Python: Process API Requests (Custom Output)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/.logging/process-api-requests.py",
      "args": ["--output-dir", "${workspaceFolder}/.logging/output", "--verbose"],
      "console": "integratedTerminal",
      "justMyCode": true,
      "python": "${workspaceFolder}/.logging/.venv/Scripts/python.exe",
      "cwd": "${workspaceFolder}",
      "env": {}
    }
  ]
}
